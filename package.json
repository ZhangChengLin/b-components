{
  "name": "b-components-js",
  "version": "0.0.1-beta.0",
  "description": "Generate Bootstrap components through JavaScript",
  "main": "",
  "scripts": {
    "bundlewatch": "bundlewatch --config .bundlewatch.config.json",
    "compile": "npm-run-all --aggregate-output -p js-compile",
    "js-compile": "npm-run-all --aggregate-output -p js-compile-*",
    "js-compile-index": "rollup --environment BsNAME:components --config buildConfig/rollup.config.js",
    "js-compile-index-esm": "rollup --environment BsNAME:components,ESM:true --config buildConfig/rollup.config.js",
    "js-compile-modal": "rollup --environment BsNAME:modal --config buildConfig/rollup.config.js",
    "js-compile-modal-esm": "rollup --environment BsNAME:modal,ESM:true --config buildConfig/rollup.config.js",
    "js-compile-offcanvas": "rollup --environment BsNAME:offcanvas --config buildConfig/rollup.config.js",
    "js-compile-offcanvas-esm": "rollup --environment BsNAME:offcanvas,ESM:true --config buildConfig/rollup.config.js",
    "js-compile-toast": "rollup --environment BsNAME:toast --config buildConfig/rollup.config.js",
    "js-compile-toast-esm": "rollup --environment BsNAME:toast,ESM:true --config buildConfig/rollup.config.js",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "js-lint-fix": "eslint --fix .",
    "lint": "npm-run-all --aggregate-output -p js-lint",
    "lint-fix": "npm-run-all --aggregate-output -p js-lint-fix",
    "release": "npm-run-all -s lint compile bundlewatch",
    "deprecatePreviousVersion": "npm deprecate b-components-js@'< 0.0.1-beta.0' 'New version has been released'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ZhangChengLin/b-components.git"
  },
  "github": "https://github.com/ZhangChengLin/b-components",
  "homepage": "https://ZhangChengLin.github.io/b-components",
  "bugs": {
    "url": "https://github.com/ZhangChengLin/b-components/issues"
  },
  "keywords": [
    "bootstrap",
    "bootstrap5",
    "components",
    "Modal",
    "Offcanvas",
    "Toast",
    "Alert",
    "Carousel",
    "Collapse",
    "Dropdown",
    "Progress",
    "Scrollspy",
    "Tab"
  ],
  "author": {
    "name": "ZhangChengLin",
    "email": "469946668@qq.com",
    "url": "https://ZhangChengLin.github.io"
  },
  "license": "MIT",
  "engines": {},
  "peerDependencies": {
    "bootstrap": "^5.x"
  },
  "dependencies": {
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "bundlewatch": "^0.3.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unicorn": "^40.1.0",
    "marked": "^4.0.12",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup": "^2.67.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.10.0"
  },
  "files": [
    "dist/**/*.{js,css,map}",
    "src/**/*.js"
  ]
}
